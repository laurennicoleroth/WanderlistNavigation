platform :ios, '12.1'

target 'Wanderlist' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'SwiftLint'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Mapbox-iOS-SDK'
  pod 'MapboxNavigation'
  pod 'GooglePlaces'
