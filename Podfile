  pod 'InstantSearchClient', '~> 6.0'
  pod 'SwiftLocation', '~> 3.2.3'
  pod 'MMBannerLayout'
  pod 'Kingfisher'

  target 'WanderlistTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
